# API Base URL - Leave empty to use the same origin as the frontend
# Example: VITE_API_BASE_URL=http://localhost:9080
VITE_API_BASE_URL=

# Registry host for Terraform provider/module usage instructions
# Development: localhost or localhost.localdomain
# Production: your domain (e.g., privatetf.local, registry.example.com)
VITE_REGISTRY_HOST=localhost

# Registry port for Terraform provider/module usage instructions
# Development: 9080
# Production: 443 (if using HTTPS) or your custom port
VITE_REGISTRY_PORT=9080
